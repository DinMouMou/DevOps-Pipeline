<?xml version='1.0' encoding='UTF-8'?>
<project>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@3.5.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.ncsu.edu/dding3/DevOps.git</url>
        <credentialsId>{{ cred_id }}</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/M3</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>mkdir -p $WORKSPACE/DevOps/Deployment/iTrustPostBuild/keys
cp $WORKSPACE/../aws.pem $WORKSPACE/DevOps/Deployment/iTrustPostBuild/keys/aws.pem
chmod 400 $WORKSPACE/DevOps/Deployment/iTrustPostBuild/keys/aws.pem
cp $WORKSPACE/../inventory_itrust $WORKSPACE/DevOps/Deployment/iTrustPostBuild/inventory
cd $WORKSPACE/DevOps/Deployment/iTrustPostBuild
ansible-playbook create_ec2_instance.yml
ansible-playbook playbook.yml -i inventory</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>