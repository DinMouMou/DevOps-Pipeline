<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.plugins.git.GitSCM" plugin="git@3.5.1">
    <configVersion>2</configVersion>
    <userRemoteConfigs>
      <hudson.plugins.git.UserRemoteConfig>
        <url>https://github.com/DinMouMou/checkbox.io</url>
		<credentialsId>932e0fc9-b0cd-4892-8903-9bd57c92898b</credentialsId>
      </hudson.plugins.git.UserRemoteConfig>
    </userRemoteConfigs>
    <branches>
      <hudson.plugins.git.BranchSpec>
        <name>*/master</name>
      </hudson.plugins.git.BranchSpec>
    </branches>
    <doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
    <submoduleCfg class="list"/>
    <extensions/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
		<command>
		
			cd ./server-side
			npm install --prefix ./analysis/;
			if node ./analysis/analysis.js ./site/marqdown.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(marqdown.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/server.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(server.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/admin.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(admin.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/create.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(create.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/csv.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(csv.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/designer.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(designer) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/live.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(live.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/study.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(study.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/studyModel.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(studyModel.js) -> Fail build&quot;; exit 1;
			else if node ./analysis/analysis.js ./site/routes/upload.js | grep -q &apos;**Fail**&apos;;
			then echo &quot;Fail analysis(upload.js) -> Fail build&quot;; exit 1;
			else npm install --prefix ./site; 
			fi
			
		</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
    <hudson.tasks.BuildTrigger>
      <childProjects>checkbox-post-build</childProjects>
      <threshold>
        <name>SUCCESS</name>
        <ordinal>0</ordinal>
        <color>BLUE</color>
        <completeBuild>true</completeBuild>
      </threshold>
    </hudson.tasks.BuildTrigger>
  </publishers>
  <buildWrappers/>
</project>